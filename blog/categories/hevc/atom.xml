</span><span class='line'>        <span class="p">}</span>
</span><span class='line'>    <span class="p">}</span><span class="o">&lt;/</span><span class="n">p</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="n">pre</span><span class="o">&gt;&lt;</span><span class="n">code</span><span class="o">&gt;*</span><span class="n">pi_depth</span> <span class="o">=</span> <span class="n">i_max_num_reorder_frames</span><span class="p">;</span>
</span><span class='line'><span class="o">*</span><span class="n">pi_delay</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
